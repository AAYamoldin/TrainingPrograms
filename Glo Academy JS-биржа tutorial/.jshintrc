{
    "camelcase": true,
    "indent": 2,
    "undent": true,
    "quotemark": false,
    "maxlen": 120,
    "trailing": true,
    "curly": true,
    "strict": false,
    "browser": true,
    "devel": true,
    "jquery": true,
    "esversion": 6,
    "node": true,
    
    "predef": [ "$", "jQuery", "text", "$p", "window", "document", "console", "parent" ],
